{
  "name": "obsidian-plugin",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && node './scripts/copy-css.js'",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "create-graph-svg": "npx depcruise src --include-only '^src' --output-type dot | dot -T svg > dependency-graph.svg",
    "create-graph-mermaid": "npx depcruise --exclude node_modules --output-type mermaid src --output-to dependency-graph.mmd"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.12.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "dependency-cruiser": "^16.3.3",
    "dprint": "^0.46.1",
    "esbuild": "^0.21.4",
    "esbuild-sass-plugin": "^3.3.0",
    "obsidian": "latest",
    "react-hook-form": "^7.51.5",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@schedule-x/calendar-controls": "^1.42.3",
    "@schedule-x/current-time": "^1.42.3",
    "@schedule-x/drag-and-drop": "^1.42.3",
    "@schedule-x/event-recurrence": "^1.42.3",
    "@schedule-x/events-service": "^1.42.3",
    "@schedule-x/react": "^1.42.0",
    "@schedule-x/resize": "^1.42.3",
    "@schedule-x/scroll-controller": "^1.42.3",
    "@schedule-x/shared": "^1.42.3",
    "@schedule-x/theme-default": "^1.42.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxdb": "^15.23.0",
    "rxdb-hooks": "^5.0.2",
    "rxjs": "^7.8.1"
  }
}
